#include <iostream>
#include <cmath>
using namespace std;
int recursiva(int a=0, int b=0);
int recursiva(int a, int b){
    int x = a+1;
	if(x==b){
	    cout << "Fin de la lista" <<endl;
	    return x;
	}else{
	    cout<<x<<endl;
	    recursiva(x, b);
	    return 0;
	}

}
int main(){
    cout << "Lista"<<endl;
	cout << "ingrese el valor de inicio A"<<endl;
	int a =0;
	cin>> a;
	cout << "ingrese el valor de fin B"<<endl;
	int b=0;
	cin>> b;
	cout<<"los enteros entre A y B son: "<<endl;
    recursiva(a,b);
}
